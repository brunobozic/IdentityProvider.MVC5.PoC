<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>Welcome To | Bootstrap Based Admin Template - Material Design</title>

    <!-- Favicon-->
    <link rel="icon" href="favicon.ico" type="image/x-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">

    <!--------------------------
    | Stylesheets here |
    -------------------------->
    <!-- Bootstrap Core Css -->
    <link href="~/bower_components/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

    <!-- Waves Effect Css -->
    @*<link href="plugins/node-waves/waves.css" rel="stylesheet" />*@
    <link href="~/bower_components/Waves/dist/waves.css" rel="stylesheet" />

    <!-- Animation Css -->
    <link href="~/bower_components/animate.css/animate.css" rel="stylesheet" />

    <!-- Morris Chart Css-->
    <link href="~/bower_components/morris.js/morris.css" rel="stylesheet" />

    <!-- Custom Css -->
    <link href="~/Content/site.css" rel="stylesheet" />

    <link href="~/bower_components/adminbsb-materialdesign/css/materialize.css" rel="stylesheet" />

    <!-- AdminBSB Themes. You can choose a theme from css/themes instead of get all themes -->
    <link href="~/bower_components/adminbsb-materialdesign/css/themes/all-themes.css" rel="stylesheet" />
    <link href="~/bower_components/adminbsb-materialdesign/css/style.css" rel="stylesheet" />
    <link href="~/bower_components/adminbsb-materialdesign/css/materialize.css" rel="stylesheet" />

    <link href="~/Content/css/themed-buttons-deep-purple.css" rel="stylesheet" />
    <link href="~/Content/css/themed-buttons-red.css" rel="stylesheet" />
    <!------------------------------
    | Very important scripts here |
    ------------------------------->
    <!-- Jquery Core Js -->
    <script src="~/bower_components/jQuery/dist/jquery.js"></script>
    <!-- Slim scroll Js -->
    <script src="~/bower_components/jquery-slimscroll/jquery.slimscroll.js"></script>
    <!-- Lodash -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/3.5.0/lodash.min.js"></script>
    @*<script src="~/bower_components/lodash/dist/lodash.js"></script>*@

    <!-- SWAL -->
    <script src="~/bower_components/sweetalert2/dist/sweetalert2.all.js"></script>
    <link href="~/bower_components/sweetalert2/dist/sweetalert2.css" rel="stylesheet" />
    <script src="~/bower_components/sweetalert2/dist/sweetalert2.js"></script>

    <link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/makeTableResponsive.js"></script>
    <link href="~/Content/MakeTableResponsive.css" rel="stylesheet" />
    <script src="~/bower_components/adminbsb-materialdesign/js/pages/ui/tooltips-popovers.js"></script>
    <link href="~/Content/popover-overrides.css" rel="stylesheet" />
    <link href="~/bower_components/select2/dist/css/select2.css" rel="stylesheet" />
    <script src="~/bower_components/select2/dist/js/select2.full.js"></script>

    <!-- Muuri -->
    <script src="https://unpkg.com/web-animations-js@2.3.1/web-animations.min.js"></script>
    <script src="https://unpkg.com/hammerjs@2.0.8/hammer.min.js"></script>
    <script src="https://unpkg.com/muuri@0.7.0/dist/muuri.min.js"></script>
</head>

<body class="theme-red">
    <!-- Page Loader -->
    @Html.Partial("_AdminBSBPageLoader");
    <!-- #END# Page Loader -->
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>
    <!-- #END# Overlay For Sidebars -->
    <!-- Search Bar -->
    @Html.Partial("_AdminBSBSearchBar");
    <!-- #END# Search Bar -->
    <!-- Top Bar -->
    @Html.Partial("_AdminBSBTopBar");
    <!-- #Top Bar -->

    <section>
        <!-- Left Sidebar -->
        @Html.Partial("_AdminBSBLeftSidebar");
        <!-- #END# Left Sidebar -->
        <!-- Right Sidebar -->
        @Html.Partial("_AdminBSBRightSidebar");
        <!-- #END# Right Sidebar -->
    </section>
    <div class="clearfix">
        <section class="content" style="margin-top:35px !important">

            @*@Html.Partial("_AdminBSBMain");*@

            <!--------------------------
            | Your Page Content Here |
            -------------------------->

            @RenderBody()

        </section>
    </div>
    <!--------------------------
    | Javascript here |
    -------------------------->
    <!-- Jquery Core Js -->
    <script src="~/bower_components/jquery/dist/jquery.js"></script>

    <!-- Bootstrap Core Js -->
    <script src="~/bower_components/bootstrap/dist/js/bootstrap.js"></script>

    <!-- Select Plugin Js -->
    <script src="~/bower_components/bootstrap-select/dist/js/bootstrap-select.js"></script>

    <!-- Slimscroll Plugin Js -->
    <script src="~/bower_components/jquery-slimscroll/jquery.slimscroll.js"></script>

    <!-- Waves Effect Plugin Js -->
    <script src="~/bower_components/Waves/dist/waves.js"></script>

    <!-- Jquery CountTo Plugin Js -->
    <script src="~/bower_components/jquery-countTo/jquery.countTo.js"></script>

    <!-- Morris Plugin Js -->
    <script src="~/bower_components/raphael/raphael.js"></script>
    <script src="~/bower_components/morris.js/morris.js"></script>

    <!-- ChartJs -->
    <script src="~/bower_components/Chart.js/Chart.js"></script>

    <!-- Flot Charts Plugin Js -->
    @*<script src="~/bower_components/Flot/jquery.flot.js"></script>
        <script src="~/bower_components/Flot/jquery.flot.resize.js"></script>
        <script src="~/bower_components/Flot/jquery.flot.pie.js"></script>
        <script src="~/bower_components/Flot/jquery.flot.categories.js"></script>
        <script src="~/bower_components/Flot/jquery.flot.time.js"></script>*@

    <!-- Sparkline Chart Plugin Js -->
    <script src="~/bower_components/jquery-sparkline/dist/jquery.sparkline.js"></script>

    <!-- Custom Js -->
    <script src="~/bower_components/adminbsb-materialdesign/js/admin.js"></script>
    <script src="~/bower_components/adminbsb-materialdesign/js/pages/index.js"></script>
    <script src="~/bower_components/adminbsb-materialdesign/js/demo.js"></script>

    <!-- Sparkline Js -->
    <script src="~/bower_components/jquery-sparkline/dist/jquery.sparkline.js"></script>

    <!-- Select2 Js -->
    <script src="~/bower_components/select2/dist/js/select2.js"></script>
    <link href="~/bower_components/select2/dist/css/select2.css" rel="stylesheet" />

    <!-- Boolean checkboxes jQuery override -->
    <script src="~/Scripts/jQueryOverride-boolean-checkboxes.js"></script>


</body>
</html>